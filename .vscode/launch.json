    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Train Informer",
            "type": "debugpy",
            "request": "launch",
            "program": "main_informer.py",
            "console": "integratedTerminal",
            "args": [
                "--model", "informerstack",
                // Dataset
                "--data", "DongWoon",
                "--root_path", "MIX50K",
                "--features", "MS",
                "--target", "Pos",
                "--seq_len", "512",
                "--label_len", "50",
                "--pred_len", "50",
                "--c_out", "1",
                // Model Parameters
                "--d_model", "256",
                "--n_heads", "8",
                "--e_layers", "2",
                "--d_layers", "1",
                "--s_layers", "3,1",
                "--d_ff", "1024",
                "--distil",
                "--embed", "fixed",
                // Training Parameters
                "--train_epochs", "100",
                "--batch_size", "32",
                "--patience", "10",
                "--learning_rate", "1e-4",
                "--loss", "mse",
                "--use_gpu", "True",
                "--gpu", "0",
            ],
            "justMyCode": false
        },
    ]
}